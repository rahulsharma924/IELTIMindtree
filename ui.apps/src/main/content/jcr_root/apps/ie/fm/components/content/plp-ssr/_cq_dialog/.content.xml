<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Product SSR Page Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.embed.v2.editor]">
    <content
        granite:class="cmp-embed__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <plp
                        jcr:primaryType="nt:unstructured"
                        jcr:title="PLP Section"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <datasheetrootpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Data Sheet Root Path"
                                                fieldLabel="Data Sheet Root Path"
                                                name="./datasheetrootpath"
                                                required="{Boolean}true"
                                                rootPath="/content/dam/fm"/>
                                            <productimagerootpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Product Image Root Path"
                                                fieldLabel="Product Image Root Path"
                                                name="./productimagerootpath"
                                                required="{Boolean}true"
                                                rootPath="/content/dam/fm"/>
                                            <viewproduct
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter View Product Button text"
                                                    fieldLabel="View Product Button text"
                                                    name="./viewproduct"
                                                    required="{Boolean}true"/>
                                            <freightdescription
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter freight description text"
                                                    fieldLabel="Freight Description"
                                                    name="./freightdescription"
                                                    required="{Boolean}true"/>
                                            <freighttitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter freight title text"
                                                    fieldLabel="Freight text"
                                                    name="./freighttitle"
                                                    required="{Boolean}true"/>
                                            <new
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter new text"
                                                    fieldLabel="New text"
                                                    name="./new"
                                                    required="{Boolean}true"/>
                                            <defaultText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter sort by default text"
                                                    fieldLabel="Sort by default text"
                                                    name="./defaultText"
                                                    required="{Boolean}true"/>
                                            <nameAscText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter sort by name Ascending text"
                                                    fieldLabel="Sort by name Ascending text"
                                                    name="./nameAscText"
                                                    required="{Boolean}true"/>
                                            <nameDescText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter sort by name descending text"
                                                    fieldLabel="Sort by name descending text"
                                                    name="./nameDescText"
                                                    required="{Boolean}true"/>
                                            <priceAscText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter sort by price Ascending text"
                                                    fieldLabel="Sort by price Ascending text"
                                                    name="./priceAscText"
                                                    required="{Boolean}true"/>
                                            <priceDescText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter sort by price descending text"
                                                    fieldLabel="Sort by price descending text"
                                                    name="./priceDescText"
                                                    required="{Boolean}true"/>
                                            <newsreleasetitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                    checked="{Boolean}true"
                                                    fieldDescription="Please check the box to display new releases page information"
                                                    name="./newsreleasetitle"
                                                    text="New Releases"
                                                    uncheckedValue="{Boolean}false"
                                                    value="{Boolean}true"/>
                                            <bestsellertitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                    checked="{Boolean}true"
                                                    fieldDescription="Please check the box to display best seller page information"
                                                    name="./bestsellertitle"
                                                    text="Best Seller"
                                                    unchecked="{Boolean}true"
                                                    value="true"/>
                                            <newReleasesBreadcrumbText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter new releases breadcrumb text"
                                                    fieldLabel="NewReleases Breadcrumb text"
                                                    name="./newReleasesBreadcrumbText"/>
                                            <bestSellarBreadcrumbText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter best sellar breadcrumb text"
                                                    fieldLabel="BestSellar Breadcrumb text"
                                                    name="./bestSellarBreadcrumbText"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </plp>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
