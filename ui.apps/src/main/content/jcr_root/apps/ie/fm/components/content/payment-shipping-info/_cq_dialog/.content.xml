<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Custom Capabilities"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_text_v2"
    trackingFeature="core-components:text:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <payment
                        jcr:primaryType="nt:unstructured"
                        jcr:title="payment"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <shippingaddressLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="shipping address title"
                                                name="./shipping"/>
                                            <frieghtforwardaddressLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="frieght address Title"
                                                name="./frieghtforwardaddressLabel"/>
                                            <paymentTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="payment Title"
                                                name="./paymentTitle"/>
                                            <editLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Edit shipping"
                                                fieldLabel="Button"
                                                name="./editlabel"/>
                                            <poLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Purchase order label"
                                                fieldLabel="Purchase order label"
                                                name="./poLabel"/>
                                            <wireTransferLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="wire transfer label"
                                                fieldLabel="wire transfer label"
                                                name="./wireTransferLabel"/>
                                            <creditCardLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="credit or debit card label"
                                                fieldLabel="credit or debit card label"
                                                name="./creditCardLabel"/>
                                            <creditCardNumber
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Credit Card Number"
                                                name="./creditCardNumber"/>
                                            <expirationDate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Expiration Date Label"
                                                name="./expirationDate"/>
                                            <cvv
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="cvv label"
                                                name="./cvv"/>
                                            <saveCardLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="save card Title"
                                                name="./saveCardLabel"/>
                                            <billingAddressLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="billing address label"
                                                name="./billingAddressLabel"/>
                                            <billingShippingSame
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="billing address same as shipping label"
                                                name="./billingShippingSame"/>
                                            <continueLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Continue button label"
                                                name="./continueLabel"/>
                                            <saveButtonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="save button label"
                                                name="./saveButtonLabel"/>
                                            <otherAddressLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="other address label"
                                                    name="./otherAddressLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </payment>
                    <editaddress
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Edit Address"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <addNewAddress
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter name label"
                                                fieldLabel="add new address label"
                                                name="./addNewAddress"/>
                                            <addBillingAddress
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter name label"
                                                    fieldLabel="add billing address label"
                                                    name="./addBillingAddress"/>
                                            <editAddressLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter name label"
                                                fieldLabel="edit Address Label"
                                                name="./editAddress"/>
                                            <name
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter name label"
                                                fieldLabel="Enter Name Label"
                                                name="./editaddressname"/>
                                            <companyname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the company"
                                                fieldLabel="Enter Company Name Label"
                                                name="./editaddresscompany"/>
                                            <phonenumber
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter phonenumber label"
                                                fieldLabel="Enter Phonenumber Label"
                                                name="./editaddressphonenumber"/>
                                            <country
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter country label"
                                                fieldLabel="Enter Country Label"
                                                name="./editaddresscountry"/>
                                            <addressline1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter address1 label"
                                                fieldLabel="Enter Address1 Label"
                                                name="./editaddress1"/>
                                            <addressline2
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter address2 label"
                                                fieldLabel="Enter Address2 Label"
                                                name="./editaddressoptional"/>
                                            <city
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter city label"
                                                fieldLabel="Enter City Label"
                                                name="./editaddresscity"/>
                                            <state
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter state label"
                                                fieldLabel="Enter State Label"
                                                name="./editaddressstate"/>
                                            <zipcode
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter zipcode label"
                                                fieldLabel="Enter Zipcode Label"
                                                name="./editaddresszipcode"/>
                                            <email
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter email label"
                                                fieldLabel="Enter Email Label"
                                                name="./editaddressemail"/>
                                            <save-btn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Save Button"
                                                fieldLabel="Save Button"
                                                name="./editaddresssavebtn"/>
                                            <cancel-btn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Cancel Button"
                                                fieldLabel="Cancel Button"
                                                name="./editaddresscancelbtn"/>
                                            <delete-btn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Delete Button"
                                                fieldLabel="Delete Button"
                                                name="./editaddressdeletebtn"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </editaddress>
                    <livesupport
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Live Support"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="Title"
                                                name="./titleone"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the description"
                                                fieldLabel="Description"
                                                name="./descriptionone"/>
                                            <logo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the logo"
                                                fieldLabel="logo"
                                                name="./logoone"
                                                rootPath="/content/dam/projects/ie"/>
                                            <buttontext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Button text"
                                                fieldLabel="Button"
                                                name="./buttonone"/>
                                            <buttonlink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the button link"
                                                fieldLabel="Link"
                                                name="./buttonlinkone"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </livesupport>
                    <customCapabilities
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Custom Capabilities"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="Title"
                                                name="./titletwo"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the description"
                                                fieldLabel="Description"
                                                name="./descriptiontwo"/>
                                            <logo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the Logo"
                                                fieldLabel="Logo"
                                                name="./logotwo"
                                                rootPath="/content/dam/projects/ie"/>
                                            <buttontext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Button text"
                                                fieldLabel="Button"
                                                name="./buttontwo"/>
                                            <buttonlink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the button link"
                                                fieldLabel="Link"
                                                name="./buttonlinktwo"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </customCapabilities>
                    <sameDayShipping
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Same day Shipping"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="Title"
                                                name="./titlethree"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the description"
                                                fieldLabel="Description"
                                                name="./descriptionthree"/>
                                            <logo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the logo"
                                                fieldLabel="Logo"
                                                name="./logothree"
                                                rootPath="/content/dam/projects/ie"/>
                                            <buttontext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Button text"
                                                fieldLabel="Button"
                                                name="./buttonthree"/>
                                            <buttonlink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the button link"
                                                fieldLabel="Link"
                                                name="./buttonlinkthree"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </sameDayShipping>
                    <shipping-info-tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Checkout Special Instructions"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <specialinstructions-title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title"
                                                fieldLabel="Enter title"
                                                name="./title"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </shipping-info-tab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
