<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Dialog for Component"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_text_v2"
    trackingFeature="core-components:text:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <firsttab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Customer Service Contacts"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <customerservicetitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Customer Service title "
                                                fieldLabel="Title"
                                                name="./custservtitle"/>
                                            <tollfreetitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Title For Toll Free Number"
                                                fieldLabel="Title For Toll Free Number"
                                                name="./tollfreetitle"/>
                                            <tollfreenum
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Toll Free Number"
                                                fieldLabel="Enter Toll Free Number"
                                                name="./tollfreenum"/>
                                            <internphonenumtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Title For International Phone Number"
                                                fieldLabel="Title For International Phone Number"
                                                name="./interphonenumtitle"/>
                                            <internphonenum
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="International Phone Number"
                                                fieldLabel="International Phone Number"
                                                name="./interphonenum"/>
                                            <faxnumtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Fax Number Title"
                                                fieldLabel="Fax Number Title"
                                                name="./faxnumtitle"/>
                                            <faxnum
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Fax Number"
                                                fieldLabel="Fax Number"
                                                name="./faxnum"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </firsttab>
                    <secondtab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Email Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <emailtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Title For Email Contact"
                                                fieldLabel="Title For Email Contact"
                                                name="./emailtitle"/>
                                            <salesemailtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Sales Email Title"
                                                fieldLabel="Sales Email Title"
                                                name="./salesmailtitle"/>
                                            <salesmail
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Sales Email"
                                                fieldLabel="Sales Email"
                                                name="./salesmail"/>
                                            <techmailtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Technical Support Mail Title"
                                                fieldLabel="Technical Support Mail Title"
                                                name="./techmailtitle"/>
                                            <techsupportmail
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Technical Support Mail"
                                                fieldLabel="Technical Support Mail"
                                                name="./techsupportmail"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </secondtab>
                    <thirdtab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Address &amp; Office Hours"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <offchourtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Office Hours Title"
                                                fieldLabel="Office Hours Title"
                                                name="./offchourtitle"/>
                                            <offchours
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Office Hours"
                                                fieldLabel="Office Hours"
                                                name="./offchours"/>
                                            <addresstitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Title For Address"
                                                fieldLabel="Title For Address"
                                                name="./addresstitle"/>
                                            <address
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Address"
                                                fieldLabel="Address"
                                                name="./address"/>
                                            <maplink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Map Link"
                                                fieldLabel="Map Link"
                                                name="./maplink"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </thirdtab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
