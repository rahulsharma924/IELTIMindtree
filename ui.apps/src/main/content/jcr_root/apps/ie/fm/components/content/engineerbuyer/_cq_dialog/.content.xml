<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Dialog for Component"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_text_v2"
    trackingFeature="core-components:text:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <Engineer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Engineer"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="Title"
                                                name="./titleone"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the description"
                                                fieldLabel="Description"
                                                name="./descriptionone"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the Image"
                                                fieldLabel="Image"
                                                name="./image"
                                                rootPath="/content/dam/projects/ie"/>
                                            <iconnameone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon name"
                                                fieldLabel="Icon Text "
                                                name="./iconnameone"/>
                                            <iconlinkone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon link"
                                                fieldLabel="Link"
                                                name="./iconlinkone"/>
                                            <iconnametwo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon name"
                                                fieldLabel="Icon Text"
                                                name="./iconnametwo"/>
                                            <iconlinktwo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon link"
                                                fieldLabel="Link"
                                                name="./iconlinktwo"/>
                                            <iconnamethree
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon name"
                                                fieldLabel="Icon Text"
                                                name="./iconnamethree"/>
                                            <iconlinkthree
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon link"
                                                fieldLabel="Link"
                                                name="./iconlinkthree"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </Engineer>
                    <Buyer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Buyer"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the title "
                                                fieldLabel="Title"
                                                name="./titletwo"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the description"
                                                fieldLabel="Description"
                                                name="./descriptiontwo"/>
                                            <imagebuyer
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Enter the Image"
                                                fieldLabel="Image"
                                                name="./imagebuyer"
                                                rootPath="/content/dam/projects/ie"/>
                                            <iconnameone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon name"
                                                fieldLabel="Icon Text"
                                                name="./iconnameoneb"/>
                                            <iconlinkone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon link"
                                                fieldLabel="Link"
                                                name="./iconlinkoneb"/>
                                            <iconnametwo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon name"
                                                fieldLabel="Icon Text"
                                                name="./iconnametwob"/>
                                            <iconlinktwo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon link"
                                                fieldLabel="Link"
                                                name="./iconlinktwob"/>
                                            <iconnamethree
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon name"
                                                fieldLabel="Icon Text"
                                                name="./iconnamethreeb"/>
                                            <iconlinkthree
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Icon link"
                                                fieldLabel="Link"
                                                name="./iconlinkthreeb"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </Buyer>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
